    "babel-preset-gatsby-package": "^2.4.0",
    "gatsby": "5.2.0",
    "standard-version": "^9.3.2"
  },
  "peerDependencies": {
    "gatsby": "5.2.0",
    "gatsby-source-shopify": "^4.0.0",
    "i18next": "^20.x || ^21.x",
